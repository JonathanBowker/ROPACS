@prefix ns1: <http://purl.org/dc/terms/> .
@prefix ns2: <http://w3.org/ns/dpv#> .
@prefix ns3: <http://www.w3.org/2006/time#> .
@prefix ns4: <http://www.w3.org/ns/org#> .
@prefix ns5: <http://xmlns.com/foaf/0.1/> .
@prefix ns6: <http://www.w3.org/ns/dcat#> .
@prefix ns7: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/alpha/CustomerService> a ns4:OrganizationalUnit,
        ns5:Agent ;
    ns4:subOrganizationOf <http://example.com/alpha/Alpha> ;
    ns5:name "Customer Service Dept." .

<http://example.com/alpha/HR> a ns4:OrganizationalUnit,
        ns5:Agent ;
    ns4:subOrganizationOf <http://example.com/alpha/Alpha> ;
    ns5:name "HR Dept." .

<http://example.com/alpha/ProcessingRecords> a ns6:Catalog,
        ns6:Dataset ;
    ns1:contactPoint [ a ns2:DataProtectionOfficer,
                <http://www.w3.org/2006/vcard/ns#Kind>,
                ns5:Agent ;
            ns2:hasContact "dpo@example.com" ;
            ns7:sameAs <http://example.com/alpha/DPO> ;
            ns5:name "Jane Doe" ] ;
    ns1:description "Organisation level catalog of processing records collated from all departments and contractors" ;
    ns1:publisher [ a ns2:Controller,
                <http://www.w3.org/ns/adms#Company>,
                ns4:Organization,
                ns5:Agent ;
            ns7:sameAs <http://example.com/alpha/Alpha> ;
            ns5:name "Alpha Inc." ] ;
    ns1:title "Processing Records" ;
    ns6:dataset <http://example.com/alpha/TestDataset> .

<http://example.com/alpha/marketing/recordscatalog> a ns6:Catalog,
        ns6:Dataset ;
    ns1:creator <http://example.com/alpha/Marketing> ;
    ns1:description "Collated records for Marketing Department" ;
    ns1:issued "2021-03-28"^^xsd:date ;
    ns1:publisher <http://example.com/alpha/Alpha> ;
    ns1:title "Processing Records for Marketing" ;
    ns6:dataset <http://example.com/alpha/marketing/Record001>,
        <http://example.com/alpha/marketing/Record002>,
        <http://example.com/alpha/marketing/Record003> .

<http://example.com/alpha/TestDataset> a ns6:Dataset ;
    ns1:description "dataset for testing file" ;
    ns1:title "test dataset" .

<http://example.com/alpha/marketing/Record001> a ns6:Dataset ;
    ns1:creator <http://example.com/alpha/Marketing> ;
    ns1:description "Processing Activities Record001" ;
    ns1:publisher <http://example.com/alpha/Alpha> ;
    ns1:spatial [ a ns1:Location ;
            ns1:title "EU" ] ;
    ns1:temporal [ a ns1:PeriodOfTime ;
            ns1:endDate "2001-12-13"^^xsd:date ;
            ns1:startDate "2001-01-01"^^xsd:date ] ;
    ns1:title "Record001" ;
    ns2:hasDataSubject <http://example.com/alpha/Customers> ;
    ns2:hasDuration [ a ns2:StorageDuration,
                ns3:GeneralDurationDescription ;
            ns3:years "1"^^xsd:deciman ] ;
    ns2:hasPersonalDataCategory ns2:EmailAddress ;
    ns2:hasPurpose [ a ns2:DirectMarketing ;
            ns1:title "Direct Marketing via Email" ] ;
    ns2:hasTechnicalOrganisationalMeasures <http://example.com/alpha/StandardTechnicalMeasures> ;
    rdfs:comment "test comment for closing instance data" ;
    ns6:contactPoint [ a <http://www.w3.org/2006/vcard/ns#Kind>,
                ns5:Agent ;
            ns2:hasContact "emily@example.com" ;
            ns5:name "Emily" ] .

<http://example.com/alpha/marketing/Record002> a ns6:Dataset ;
    ns1:creator <http://example.com/alpha/Marketing> ;
    ns1:description "Processing Activities Record002" ;
    ns1:publisher <http://example.com/alpha/Alpha> ;
    ns1:spatial [ a ns1:Location ;
            ns1:title "EU" ] ;
    ns1:temporal [ a ns1:PeriodOfTime ;
            ns1:endDate "2002-12-13"^^xsd:date ;
            ns1:startDate "2002-01-01"^^xsd:date ] ;
    ns1:title "Record002" ;
    ns2:hasDataSubject <http://example.com/alpha/Customers> ;
    ns2:hasDuration [ a ns2:StorageDuration,
                ns3:GeneralDurationDescription ;
            ns3:years "1"^^xsd:deciman ] ;
    ns2:hasPersonalDataCategory ns2:TelephoneNumber ;
    ns2:hasPurpose [ a ns2:DirectMarketing ;
            ns1:title "Direct Marketing via Telephone" ] ;
    ns2:hasTechnicalOrganisationalMeasures <http://example.com/alpha/StandardTechnicalMeasures> ;
    rdfs:comment "test comment for closing instance data" ;
    ns6:contactPoint [ a <http://www.w3.org/2006/vcard/ns#Kind>,
                ns5:Agent ;
            ns2:hasContact "emily@example.com" ;
            ns5:name "Emily" ] .

<http://example.com/alpha/marketing/Record003> a ns6:Dataset ;
    ns1:creator <http://example.com/alpha/Marketing> ;
    ns1:description "Processing Activities Record003" ;
    ns1:publisher <http://example.com/alpha/Alpha> ;
    ns1:spatial [ a ns1:Location ;
            ns1:title "EU" ] ;
    ns1:temporal [ a ns1:PeriodOfTime ;
            ns1:endDate "2003-12-13"^^xsd:date ;
            ns1:startDate "2003-01-01"^^xsd:date ] ;
    ns1:title "Record003" ;
    ns2:hasDataSubject <http://example.com/alpha/Customers> ;
    ns2:hasDuration [ a ns2:StorageDuration,
                ns3:GeneralDurationDescription ;
            ns3:years "1"^^xsd:deciman ] ;
    ns2:hasPersonalDataCategory ns2:PhysicalAddress ;
    ns2:hasPurpose [ a ns2:DirectMarketing ;
            ns1:title "Direct Marketing via Post" ] ;
    ns2:hasTechnicalOrganisationalMeasures <http://example.com/alpha/StandardTechnicalMeasures> ;
    rdfs:comment "test comment for closing instance data" ;
    ns6:contactPoint [ a <http://www.w3.org/2006/vcard/ns#Kind>,
                ns5:Agent ;
            ns2:hasContact "emily@example.com" ;
            ns5:name "Emily" ] .

<http://example.com/alpha/DPO> a ns2:DataProtectionOfficer,
        ns5:Agent ;
    ns2:hasContact "dpo@example.com" ;
    ns5:name "Jane Doe" .

<http://example.com/alpha/Customers> rdfs:subClassOf ns2:DataSubject .

<http://example.com/alpha/StandardTechnicalMeasures> a ns2:TechnicalOrganisationalMeasures .

<http://example.com/alpha/Marketing> a ns4:OrganizationalUnit,
        ns5:Agent ;
    ns4:subOrganizationOf <http://example.com/alpha/Alpha> ;
    ns5:name "Marketing Dept." .

<http://example.com/alpha/Alpha> a ns2:Controller,
        <http://www.w3.org/ns/adms#Company>,
        ns4:Organization,
        ns5:Agent,
        ns5:Organization ;
    ns2:hasContact "alpha@example.com" ;
    ns2:hasRepresentative <http://example.com/alpha/DPO> ;
    ns5:name "Alpha Inc." .

